{
  "name": "coc-jest",
  "version": "1.0.3",
  "description": "jest extension for coc.nvim",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "engines": {
    "coc": ">=0.0.23"
  },
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoclide/coc-jest.git"
  },
  "keywords": [
    "coc.nvim",
    "jest"
  ],
  "author": "chemzqm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoclide/coc-jest/issues"
  },
  "homepage": "https://github.com/neoclide/coc-jest#readme",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jest",
      "properties": {
        "jest.terminalPosition": {
          "type": "string",
          "description": "Position of terminal window.",
          "default": "right",
          "enum": [
            "bottom",
            "right"
          ]
        },
        "jest.watch": {
          "type": "boolean",
          "default": true
        },
        "jest.detectLeaks": {
          "type": "boolean",
          "default": false
        },
        "jest.watchman": {
          "type": "boolean",
          "default": false
        },
        "jest.detectOpenHandles": {
          "type": "boolean",
          "default": false
        },
        "jest.forceExit": {
          "type": "boolean",
          "default": false
        },
        "jest.noStackTrace": {
          "type": "boolean",
          "default": false
        },
        "jest.configFileName": {
          "type": "string",
          "default": "jest.config.js"
        }
      }
    },
    "commands": [
      {
        "title": "Run jest for current project.",
        "category": "Jest",
        "command": "jest.projectTest"
      },
      {
        "title": "Run jest for current file",
        "category": "Jest",
        "command": "jest.fileTest"
      },
      {
        "title": "Run jest init in current vim's CWD",
        "category": "Jest",
        "command": "jest.init"
      }
    ]
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.17",
    "@types/node": "^10.12.9",
    "@types/which": "^1.3.1",
    "coc.nvim": "^0.0.32",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.0.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "which": "^1.3.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {}
}
